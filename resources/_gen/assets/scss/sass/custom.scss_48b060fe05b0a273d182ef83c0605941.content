.navbar-nav{margin-left:auto}.footer{position:fixed;bottom:0;left:0;width:100%;height:60px;line-height:60px;padding-left:.75rem;padding-right:.75rem}@media(min-width:576px){.carousel-item img{width:auto!important}}.flex-container{list-style:none}.flex-container img{max-height:100%;min-width:100%;object-fit:cover;vertical-align:bottom}.summary img{max-height:100%;min-width:100%;max-width:480px;object-fit:cover;vertical-align:bottom}.overlay-container{position:relative}.overlay{position:absolute;bottom:1.5rem;background:#000;background:rgba(0,0,0,.25);color:#f1f1f1;width:100%;transition:.5s ease;opacity:0;color:#fff;font-size:20px;padding:20px;text-align:center}.overlay-container:hover .overlay{opacity:1}.overlay-summary{bottom:3rem;background-color:rgba(0,0,0,.25);color:#fff;font-size:32px;padding:32px 64px}.middle{transition:.5s ease;opacity:0;position:absolute;top:50%;left:50%;transform:translate(-50%,-50%);-ms-transform:translate(-50%,-50%);text-align:center}.overlay-container:hover .img-fluid{opacity:.3}.overlay-container:hover .middle{opacity:1}